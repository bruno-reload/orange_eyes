; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="orange_eyes"
run/main_scene="res://scene/main.tscn"
boot_splash/image="res://import/orange.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.00784314, 0.0980392, 0.00392157, 1 )
config/icon="res://import/orange.png"

[autoload]

scene_load="*res://scene_load.gd"

[display]

window/size/test_width=1024
window/size/test_height=600
window/stretch/mode="viewport"

[importer_defaults]

ogg_vorbis={
"loop": true,
"loop_offset": 10.0
}
wav={
"compress/mode": 0,
"edit/loop": true,
"edit/normalize": true,
"edit/trim": true,
"force/8_bit": false,
"force/max_rate": false,
"force/max_rate_hz": 44100,
"force/mono": false
}
texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 2,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": true,
"flags/mipmaps": true,
"flags/repeat": true,
"flags/srgb": 1,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": true,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[physics]

3d/physics_engine="Bullet"
3d/default_gravity_vector=Vector3( 0, 1, 0 )

[rendering]

quality/driver/driver_name="GLES2"
quality/driver/fallback_to_gles2=true
vram_compression/import_etc=true
